{
  "hooks": {
    "SubagentStop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "python3 \"$CLAUDE_PROJECT_DIR\"/.claude/hooks/traffic_controller.py",
            "timeout": 60
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "rust-analyzer-lsp@claude-plugins-official": true,
    "claude-mem@thedotmack": false,
    "pyright-lsp@claude-plugins-official": true,
    "plugin-dev@claude-plugins-official": true,
    "nsn-testnet@matt-plugins": true
  },
  "extraKnownMarketplaces": {
    "matt-plugins": {
      "source": {
        "source": "directory",
        "path": "/home/matt/plugins"
      }
    }
  }
}
